{
  "name": "resume-parser",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon ./server.js",
    "start": "NODE_ENV=production node ./server.js",
    "migrate": "knex --knexfile ./knexfile.js migrate:latest",
    "seed": "knex --knexfile ./knexfile.js seed:run",
    "knex:prod": "NODE_ENV=production knex --knexfile ./knexfile.js"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mammoth": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.6",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
