{
  "name": "resume-parser",
  "version": "1.0.0",
  "type": "module",
  "main": "server/server.js",
  "scripts": {
    "dev": "nodemon server/server.js",
    "start": "NODE_ENV=production node server/server.js",
    "migrate": "knex --knexfile ./knexfile.js migrate:latest",
    "seed": "knex --knexfile ./knexfile.js seed:run",
    "knex:prod": "NODE_ENV=production knex --knexfile ./knexfile.js"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
